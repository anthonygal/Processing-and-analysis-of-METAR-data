   "source": [
    "def data_file_generator(data_path):\n",
    "    with open(data_path, 'r') as f: \n",
    "        reader = csv.DictReader(f, delimiter = ',')\n",
    "        for r in reader:\n",
    "            yield dict(r)"
   ]
  },
  {
